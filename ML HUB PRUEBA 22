    end
end)
FarmTab:AddSwitch("Auto Teleport to Muscle King", function(v)
    _G.AutoTeleport = v
    while _G.AutoTeleport do task.wait()
        game:GetService("ReplicatedStorage"):WaitForChild("TeleportMuscleKing"):FireServer()
    end
